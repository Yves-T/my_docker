{
  "name": "docker_yarn",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./app.js",
    "dev": "nodemon --inspect=0.0.0.0:9229 ./app.js",
    "test": "jest",
    "watch-tests": "jest --watchAll",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "mongoose": "^5.0.0-rc0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.15.0",
    "eslint-watch": "^3.1.3",
    "jest": "^22.0.6",
    "nodemon": "^1.14.7",
    "supertest": "^3.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": ["<rootDir>/node_modules/"]
  },
  "babel": {
    "presets": ["es2015"]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": ["error", 2],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "single"],
      "semi": ["error", "always"]
    }
  }
}
